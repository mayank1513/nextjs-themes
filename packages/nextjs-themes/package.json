{
  "name": "nextjs-themes",
  "author": "Mayank Kumar Chaudhari <https://mayank-chaudhari.vercel.app>",
  "version": "0.4.0",
  "description": "Unleash the Power of React Server Components! Use multiple themes on your site with confidence, without losing any advantages of React Server Components.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayank1513/nextjs-themes.git"
  },
  "bugs": {
    "url": "https://github.com/mayank1513/nextjs-themes/issues"
  },
  "homepage": "https://github.com/mayank1513/nextjs-themes#readme",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"**/*.ts*\"",
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "pnpm build --watch",
    "publish-package": "cp ../../README.md dist && node createPackageJSON.js && cd dist && npm publish && cd ..",
    "publish-package2": "node seo.js && cd dist && npm publish && cd ..",
    "publish-gpr": "cp ../../README.md dist && node prepGPR.js && cd dist && npm publish && cd ..",
    "publish-gpr2": "node seo.js && cd dist && npm publish",
    "test": "vitest run --coverage"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-v8": "^0.34.1",
    "esbuild-react18-useclient": "^1.0.5",
    "eslint": "^8.47.0",
    "eslint-config-custom": "workspace:*",
    "jsdom": "^22.1.0",
    "next": "^13.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1",
    "zustand": "^4.4.1"
  },
  "dependencies": {
    "persistnsync": "workspace:*"
  },
  "peerDependencies": {
    "@types/react": "^16.8 || ^17 || ^18",
    "next": "^8",
    "react": "^16.8 || ^17 || ^18"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/mayank1513"
  },
  "keywords": [
    "nextjs",
    "nextjs13",
    "nextjs-themes",
    "nextjs13-themes",
    "react",
    "react18",
    "react-themes",
    "react18-themes",
    "react-server-components",
    "react-client-components",
    "themes",
    "dark-mode",
    "dark-themes",
    "typescript",
    "javascript"
  ]
}
